{
    "name": "Master settings",
    "dataColors": ["#1A388A","#E79F25","#58B4E9","#029F73","#F0E341","#2977AE","#D55E00","#CC79A7"],
    "visualStyles": {
        "*":{
            "*":{
                "general": [{
					"responsive": true
				}],
                "background": [{
                    "show": false
                }],
                "border": [{
                    "show": false
                }],
                "title": [{
                    "show": false,
                    "fontColor": { "solid": { "color": "#545454"}},
                    "background": { 
                        "show": false
                    },
                    "alignment": "left",
                    "fontSize": 14,
                    "fontFamily": "Verdana"
                }],
                "legend": [{
					"showTitle": true,
					"labelColor": { "solid": { "color": "#545454"}},
					"fontFamily": "Verdana",
					"fontSize": 12
				}],
                "categoryAxis": [{
					"show": true,
					"position": "Left",
					"labelColor": { "solid": { "color": "#8A8A8A"}},
					"fontSize": 12,
					"fontFamily": "Arial",
					"preferredCategoryWidth": 30,
					"maxMarginFactor": 28,
					"innerPadding": 22,
					"showAxisTitle": true,
					"titleColor": { "solid": { "color": "#8A8A8A"}},
					"titleFontSize": 12,
					"titleFontFamily": "Arial"
				}],
                "valueAxis": [{
					"show": true,
					"axisScale": "Linear",
					"labelColor": { "solid": { "color": "#8A8A8A"}},
					"fontSize": 12,
					"fontFamily": "Arial",
					"labelDisplayUnits": "0",
					"labelPrecision": "Auto",
					"showAxisTitle": true,
					"titleColor": { "solid": { "color": "#8A8A8A"}},
					"titleFontSize": 12,
					"titleFontFamily": "Arial",
					"gridlineShow": true,
					"gridlineColor": { "solid": { "color": "#8A8A8A"}},
					"gridlineThickness": 1,
					"gridlineStyle": "dashed"
				}]
            }
        },
        "clusteredBarChart": {
			"*": {
                "labels": [{
					"show": true,
					"color": { "solid": { "color": "#FFFFFF"}},
					"labelDisplayUnits": "0",
					"labelPrecision": 1,
					"labelPosition": "InsideEnd",
					"fontSize": 12,
					"fontFamily": "Arial",
					"enableBackground": false
				}],
                "dataPoint": [{
					"defaultColor": { "solid": { "color": "#1A388A"}}
				}]
            }
        },
        "barChart": {
			"*": {
                "labels": [{
					"show": true,
					"color": { "solid": { "color": "#FFFFFF"}},
					"labelDisplayUnits": "0",
					"labelPrecision": 1,
					"labelPosition": "InsideEnd",
					"fontSize": 12,
					"fontFamily": "Arial",
					"enableBackground": false
				}],
                "dataPoint": [{
					"defaultColor": { "solid": { "color": "#1A388A"}}
				}]
            }
        },
        "hundredPercentStackedBarChart": {
			"*": {
                "labels": [{
					"show": true,
					"color": { "solid": { "color": "#FFFFFF"}},
					"labelDisplayUnits": "0",
					"labelPrecision": 1,
					"labelPosition": "InsideEnd",
					"fontSize": 12,
					"fontFamily": "Arial",
					"enableBackground": false
				}],
                "legend": [{
                    "show" : true,
					"position": "BottomCenter"
				}]
            }
        },
        "columnChart": {
            "*": {
                "labels": [{
					"show": true,
					"color": { "solid": { "color": "#FFFFFF"}},
					"labelDisplayUnits": "0",
					"labelPrecision": 1,
					"labelPosition": "InsideEnd",
					"fontSize": 12,
					"fontFamily": "Arial",
					"enableBackground": false
				}],
                "dataPoint": [{
					"defaultColor": { "solid": { "color": "#1A388A"}}
				}]
            }
        },
        "clusteredColumnChart": {
            "*": {
                "labels": [{
					"show": true,
					"color": { "solid": { "color": "#FFFFFF"}},
					"labelDisplayUnits": "0",
					"labelPrecision": 1,
					"labelPosition": "InsideEnd",
					"fontSize": 12,
					"fontFamily": "Arial",
					"enableBackground": false
				}],
                "dataPoint": [{
					"defaultColor": { "solid": { "color": "#1A388A"}}
				}]
            }
        },
        "hundredPercentStackedColumnChart": {
			"*": {
                "labels": [{
					"show": true,
					"color": { "solid": { "color": "#FFFFFF"}},
					"labelDisplayUnits": "0",
					"labelPrecision": 1,
					"labelPosition": "InsideEnd",
					"fontSize": 12,
					"fontFamily": "Arial",
					"enableBackground": false
				}],
                "legend": [{
                    "show" : true,
					"position": "RightCenter"
				}]
            }
        },
        "lineChart": {
			"*": {
				"title": [{
                    "show": false
				}],
                "legend": [{
					"show": true,
					"position": "Bottom",
					"showTitle": true,
					"labelColor": { "solid": { "color": "#545454"}},
					"fontFamily": "Arial",
					"fontSize": 12,
					"legendMarkerRendering": "markerOnly",
					"matchLineColor": false
				}],
                "labels": [{
					"show": false,
					"color": { "solid": { "color": "#545454"}},
					"labelDisplayUnits": "0",
					"labelPrecision": 1,
					"fontSize": 12,
					"fontFamily": "Arial",
					"enableBackground": false,
					"backgroundColor": { "solid": { "color": "#0928DD"}},
					"backgroundTransparency": 30
                }],
                "categoryAxis": [{
                    "gridlineShow": true,
					"gridlineColor": { "solid": { "color": "#8A8A8A"}},
					"gridlineThickness": 1,
					"gridlineStyle": "dashed"
                }]
            }
        },
		"tableEx": {
			"*": {
				"stylePreset": [{ 
					"name": "None"
				}],
				"columnHeaders": [{
					"fontColor": { "solid": { "color": "#333333"}},
					"outline": "None",
					"autoSizeColumnWidth": true,
					"fontFamily": "Verdana",
					"fontSize": 12,
					"alignment": "Left",
					"wordWrap": true
				}],
				"values": [{
					"fontColorPrimary": { "solid": { "color": "#1A388A"}},
					"fontColorSecondary": { "solid": { "color": "#1A388A"}},
					"backColorSecondary": { "solid": { "color": "#D6D6D6"}},
					"outline": "None",
					"urlIcon": true,
					"wordWrap": true,
					"fontFamily": "Arial",
					"fontSize": 12
				}],
				"total": [{
					"totals": true,
					"fontColor": { "solid": { "color": "#545454"}},
					"outline": "None",
					"fontFamily": "Arial",
					"fontSize": 12
				}]
			}
		},
		"slicer": {
			"*": {
				"general": [{
					"outlineColor": { "solid": { "color": "#333333"}},
					"outlineWeight": 1,
					"orientation": "vertical",
					"responsive": true
				}],
				"selection": [{
					"selectAllCheckboxEnabled": false,
					"singleSelect": true
				}],
				"header": [{
					"show": true,
					"fontColor": { "solid": { "color": "#333333"}},
					"outline": "None",
					"textSize": 12,
					"fontFamily": "Arial"
				}],
				"items": [{
					"fontColor": { "solid": { "color": "#000000"}},
					"background": { "solid": { "color": "#D6D6D6"}},
					"outline": "None",
					"textSize": 12,
					"fontFamily": "Arial"
				}],
				"slider": [{ 
					"color": {"solid": { "color": "#333333"} }
				}]
			}
		},
		"card": {
			"*": {
				"labels": [{
					"color": { "solid": { "color": "#1A388A"}},
					"labelDisplayUnits": "0",
					"labelPrecision": 0,
					"fontSize": 18,
					"fontFamily": "Arial"
				}],
				"categoryLabels": [{
					"show": false
				}],
				"wordWrap": [{
					"show": true
				}],
				"title": [{
                    "show": true
				}]
			}
		},
        "page": {
            "*": {
                "background": [{
                    "color": { "solid": { "color": "#E6E6E6" } },
                    "transparency": 0
                }]
            }
        }
    }
}
